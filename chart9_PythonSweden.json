{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

    "note": "Example of simple API with no bugs",
      
    "data": {"url": "https://raw.githubusercontent.com/ee18333/ee18333.github.io/main/data_covid19api-sweden.json"},
    
    "title": {
    "text": "Sweden Covid Data",
    "subtitle": "Cummulative Covid-19 cases.  Source: Covid-19 API",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
     },
    
    "mark": "line",
    
    "encoding": {
      
      "x": {"field": "Date", "type": "temporal"},
      
      "y": {"field": "Cases", "type": "quantitative", "title": "Deaths"}
    }
}
