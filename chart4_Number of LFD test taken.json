{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": " Number of LFD test taken",
    "subtitle": "in the UK",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 12,
    "anchor": "start",
    "color": "black",
    "fontSize": 16,
    "font": "Italic"
  },
  
  "data": {
    "url": "https://raw.githubusercontent.com/ee18333/ee18333.github.io/main/cumLFTDate.csv",
    "format": {"type": "csv"}
  },
  "transform": [
    {
      "filter": {
        "field": "areaName",
        "oneOf": ["Hounslow", "Westminster","Surrey Heath"]
      }
    }
  ],
  "config": {"background": "#fffdfd", "view": {"stroke": "transparent"}},
  "height": 300,
  "width": "container",
  "mark": {"type": "line", "point": false, "strokeWidth":4},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false}
    },
    "y": {
      "field": "cumLFDTests",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    },
    "color": {
      "field": "areaName",
      "type": "nominal",
      "scale": {"scheme": "yellowgreenblue"},
      "title": null,
      "legend": {"orient": "top-left", "fillColor": "#ffffff"}
    }
  }
}
